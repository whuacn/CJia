<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!-- make sure to set 'Copy To Output Directory' option for this file -->
  <!-- go to http://nlog-project.org/wiki/Configuration_file for more information -->

  <targets>
    <!--
		<target name="console" xsi:type="Console" layout="${longdate}|${level}|${message}"/>
		-->
    <!--<target name="HealthError" xsi:type="File" fileName="C:/Health.App/${shortdate}/${windows-identity:domain=false}.log"/>-->
    <!--<target name="PrinterError" xsi:type="File" fileName="C:/WSLPrinter/Error/${shortdate}/${windows-identity:domain=false}.log"/>-->

    <target name="HealthError" xsi:type="File" fileName="log/${shortdate}/${shortdate}.log"/>
  </targets>

  <rules>
    <logger name="HealthError" minlevel="Debug" writeTo="HealthError"/>
    <!--<logger name="PrinterError" minlevel="Debug" writeTo="PrinterError"/>-->
  </rules>
</nlog>
